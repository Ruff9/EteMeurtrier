CREATE TABLE Video (
id INT(11) NOT NULL AUTO_INCREMENT,
titre VARCHAR(255) NOT NULL,
code_embed TEXT,
realisation1 VARCHAR(255),
realisation2 VARCHAR(255),
chef_operateur VARCHAR(255),
avec1 VARCHAR(255),
avec2 VARCHAR(255),
annee VARCHAR(255),
PRIMARY KEY(id)
);

